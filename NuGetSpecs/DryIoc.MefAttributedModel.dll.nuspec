<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>DryIoc.MefAttributedModel.dll</id>
    <!-- just a placeholder for "nuget pack -Version" flag -->
    <version>0.0.0</version>
    <authors>Maksim Volkau</authors>
    <copyright>Copyright © 2013 Maksim Volkau</copyright>
    <projectUrl>https://bitbucket.org/dadhi/dryioc</projectUrl>
    <licenseUrl>http://opensource.org/licenses/MIT</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>DryIoc.MefAttributedModel is DryIoc extension to support Microsoft Managed Extensibility Framework for DryIoc container</description>
    <tags>MEF Composition IoC Container Inversion-of-Control DI Dependency-Injection DRY Service-Provider Factory</tags>
    <releaseNotes>
      v4.0.3:
      - fixed: NetStandard 1.3 and 2.0 targets to use Pcl-259 profile rather than Net45
      v4.0.2:
      - fixed: Dependencies to fixed DryIocAttributes 4.0.2
      v4.0.1:
      - added: NetStandard 1.3 and 2.0 package targets
      v4.0.0:
      - Support for DryIoc v3
      - Support for DryIocAttributes v4
    </releaseNotes>
    <dependencies>
      <!--Explicitly specify no dependencies for full .NET -->
      <group targetFramework="net35">
        <dependency id="DryIoc.dll" version="3.0.1" />
        <dependency id="DryIocAttributes.dll" version="4.0.2" />
      </group>
      <group targetFramework="net40">
        <dependency id="DryIoc.dll" version="3.0.1" />
        <dependency id="DryIocAttributes.dll" version="4.0.2" />
      </group>
      <group targetFramework="net45">
        <dependency id="DryIoc.dll" version="3.0.1" />
        <dependency id="DryIocAttributes.dll" version="4.0.2" />
      </group>
      <group targetFramework="portable-net4+sl5+netcore45+wpa81+wp8+MonoAndroid1+MonoTouch1">
        <dependency id="DryIoc.dll" version="3.0.1" />
        <dependency id="DryIocAttributes.dll" version="4.0.2" />
      </group>
      <group targetFramework="portable-net45+netcore45+wpa81+wp8+MonoAndroid1+MonoTouch1">
        <dependency id="DryIoc.dll" version="3.0.1" />
        <dependency id="DryIocAttributes.dll" version="4.0.2" />
      </group>
      <group targetFramework="xamarinmac20">
        <dependency id="DryIoc.dll" version="3.0.1" />
        <dependency id="DryIocAttributes.dll" version="4.0.2" />
      </group>
      <group targetFramework="netstandard1.0">
        <dependency id="DryIoc.dll" version="3.0.1" />
        <dependency id="DryIocAttributes.dll" version="4.0.2" />
        <dependency id="NETStandard.Library" version="1.6.1" />
      </group>
      <group targetFramework="netstandard1.3">
        <dependency id="DryIoc.dll" version="3.0.1" />
        <dependency id="DryIocAttributes.dll" version="4.0.2" />
        <dependency id="NETStandard.Library" version="1.6.1" />
      </group>
      <group targetFramework="netstandard2.0">
        <dependency id="DryIoc.dll" version="3.0.1" />
        <dependency id="DryIocAttributes.dll" version="4.0.2" />
        <!--no dependencies on NETStandard.Library-->
      </group>
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.ComponentModel.Composition" targetFramework="net40" />
    </frameworkAssemblies>
