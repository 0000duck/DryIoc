            <dependency id="System.Reflection.Extensions" version="4.0.1" />
            <dependency id="System.Runtime" version="4.1.0" />
            <dependency id="System.Runtime.Extensions" version="4.1.0" />
            <dependency id="System.Threading" version="4.0.11" />
          </group>
        </dependencies>
    </metadata>
