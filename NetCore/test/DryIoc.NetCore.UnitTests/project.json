{
  "buildOptions": {
    "compile": [
      "../../../DryIoc.UnitTests/*.cs",
      "../NUnitAdapterForXUnit.cs"
    ],
    "exclude": [ "../../../DryIoc.UnitTests/ThrowTests.cs" ]
  },

  "dependencies": {
    "Microsoft.NETCore.Platforms": "1.0.1-rc2-24027",
    "DryIoc.dll": "2.6.2-netcore-rc2",
    "dotnet-test-xunit": "1.0.0-rc3-build10019",
    "xunit": "2.1.0"
  },

  "testRunner": "xunit",

  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Runtime": {
          "type": "build"
        }
      },
      "dependencies": {
        "xunit.runner.console": "2.1.0"
      }
    },
    "netcoreapp1.0": {
      "imports": [
        "dnxcore50",
        "portable-net451+win8"
      ],
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.0-rc2-3002702",
          "type": "platform"
        }
      }
    }
  }
}